#include <bits/stdc++.h>
#define lli long long int
using namespace std;

int main() {
    string s,t;
    cin>>s>>t;
    int n=s.length();
    int m=t.length();
    int count=0;
    for(int i=0;i<n;++i){
            if(s[i]==t[i]){
                count++;
            }
    }
    if(count==n){
        cout<<"Yes"<<endl;
    }
    else
    cout<<"No"<<endl;
    
	return 0;
}
